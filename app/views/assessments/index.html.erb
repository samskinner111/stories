<p id="notice"><%= notice %></p>

<h1>Assessments</h1>

<table>
  <thead>
    <tr>
      <th>Story Type</th>
      <th>Duplicate</th>
      <th>Importance</th>
      <th>Valence</th>
      <th>Match value</th>
      <th>Match strength</th>
      <th>Value violation</th>
      <th>Value violated</th>
      <th>Comments</th>
      <th>Restrict comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assessments.each do |assessment| %>
      <tr>
        <td><%= assessment.story.story_type %></td>
        <td><%= assessment.duplicate %></td>
        <td><%= assessment.importance %></td>
        <td><%= assessment.valence %></td>
        <td><%= assessment.match_value_id %></td>
        <td><%= assessment.match_strength %></td>
        <td><%= assessment.value_violation %></td>
        <td><%= assessment.value_violated_id %></td>
        <td><%= assessment.comments %></td>
        <td><%= assessment.restrict_comments %></td>
        <td><%= link_to 'Show', assessment %></td>
        <td><%= link_to 'Edit', edit_assessment_path(assessment) %></td>
        <td><%= link_to 'Destroy', assessment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assessment', new_assessment_path %>
